generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  Id String @id @default(uuid())
  Name String?
  Email String @unique
  Password String

  InTravel TravelList @relation(references: [Id],fields: [travelId])
  travelId String
}

model TravelList{
  Id String @id @default(uuid())
  Title String? 
  Where String
  From String 
  Notes String

  TravelList User[]
}

